<div class="featured-post section-blog featured-info__item">
  <div class="featured-info__item__top flex items-center justify-between">
    <h3 class="featured-info-title">{{ block.settings.heading }}</h3>
    <div class="featured-info-navigation flex items-center">
      <div class="featured-post-prev mr-2.5 featured-info-naviga-prev">
        {% render 'icon', icon_name: 'icon-prev-type1' %}
      </div>
      <div class="featured-post-next featured-info-naviga-next">
        {% render 'icon', icon_name: 'icon-next-type1' %}
      </div>
    </div>
  </div>
  <div class="swiper featured-post__list blog-list">
    <div class="swiper-wrapper">
      {% if block.settings.blog.articles != blank %}
        {%- for article in block.settings.blog.articles limit: 5 -%}
          <div class="swiper-slide blog-item featured-post__item">
            {%-
              render 'article-card-slidebar',
              show_date: block.settings.show_date,
              show_button_discover: block.settings.show_button_discover,
              article: article,
            -%}
          </div>
        {%- endfor -%}
      {% else %}
        {%- assign range = (1..4) -%}
        {% for star in range %}
          <div class="swiper-slide blog-item featured-post__item">
            <div class="blog-item-no-data">
              <div class="blog-item-no-data__image"></div>
              <div class="blog-item-no-data__content">
                <h3 class="card__heading">Blog title</h3>
                <p class="content-desc mb-4">
